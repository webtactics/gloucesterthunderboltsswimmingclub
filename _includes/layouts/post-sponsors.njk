---
layout: layouts/grid-default.njk
section: post
---


 <div class="grid-2-sponsor">
<div class="detailwrapper">

<article>
<div class="detailcontent">{{ content | safe }}</div>

{% if website %}
<ul class="infoboxmobile">
    <li><a class="social-icon" title="Follow us on Facebook" href="https://www.facebook.com/GloucesterThunderboltsSwimmingClub" target="_blank">

<svg xmlns="http://www.w3.org/2000/svg" width="30px" height="30px" viewBox="0 0 24 24"><path fill="currentColor" d="M24 12.073c0-6.627-5.373-12-12-12s-12 5.373-12 12c0 5.99 4.388 10.954 10.125 11.854v-8.385H7.078v-3.47h3.047V9.43c0-3.007 1.792-4.669 4.533-4.669c1.312 0 2.686.235 2.686.235v2.953H15.83c-1.491 0-1.956.925-1.956 1.874v2.25h3.328l-.532 3.47h-2.796v8.385C19.612 23.027 24 18.062 24 12.073z"></path></svg>

</a></li>

<li><a class="social-icon" title="Follow us on Instagram" href="https://www.instagram.com/gloucesterswimclub" target="_blank">
<svg xmlns="http://www.w3.org/2000/svg" width="30px" height="30px" viewBox="0 0 14 14"><g fill="none" stroke="currentColor" stroke-linecap="round" stroke-linejoin="round"><path d="M10.333 3.644a.25.25 0 1 1 0-.5m0 .5a.25.25 0 1 0 0-.5"></path><path d="M.858 3.431A2.573 2.573 0 0 1 3.431.858h6.862a2.573 2.573 0 0 1 2.573 2.573v6.862a2.573 2.573 0 0 1-2.573 2.573H3.43a2.573 2.573 0 0 1-2.573-2.573V3.43Z"></path><path d="M4.312 6.862a2.55 2.55 0 1 0 5.1 0a2.55 2.55 0 1 0-5.1 0"></path></g></svg>
</a></li>



<li><a class="social-icon" title="Follow us on Instagram" href="{{ website }}" target="_blank">
<svg xmlns="http://www.w3.org/2000/svg" class="ionicon" viewBox="0 0 512 512"><path d="M256 48C141.13 48 48 141.13 48 256s93.13 208 208 208 208-93.13 208-208S370.87 48 256 48z" fill="none" stroke="currentColor" stroke-miterlimit="10" stroke-width="32"/><path d="M256 48c-58.07 0-112.67 93.13-112.67 208S197.93 464 256 464s112.67-93.13 112.67-208S314.07 48 256 48z" fill="none" stroke="currentColor" stroke-miterlimit="10" stroke-width="32"/><path d="M117.33 117.33c38.24 27.15 86.38 43.34 138.67 43.34s100.43-16.19 138.67-43.34M394.67 394.67c-38.24-27.15-86.38-43.34-138.67-43.34s-100.43 16.19-138.67 43.34" fill="none" stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="32"/><path fill="none" stroke="currentColor" stroke-miterlimit="10" stroke-width="32" d="M256 48v416M464 256H48"/></svg>
</a></li>

</ul>
{% endif %}
<div class="linkwrapper">
{% if gold %}
<a class="goldsponsorwebsite" href="{{ website }}" alt="{{ title }}" title="{{ title }}" target="_blank">Website</a>
{% elif silver %}
<a class="silversponsorwebsite" href="{{ website }}" alt="{{ title }}" title="{{ title }}" target="_blank">Website</a>
{% elif bronze %}
<a class="bronzesponsorwebsite" href="{{ website }}" alt="{{ title }}" title="{{ title }}" target="_blank">Website</a>
{% endif %}
</div>
{% endif %}

{% if facebook %}
<div class="linkwrapper">
{% if gold %}
<a class="goldsponsorwebsite" href="{{ facebook }}" alt="{{ title }}" title="{{ title }}" target="_blank">Facebook</a>
{% elif silver %}
<a class="silversponsorwebsite" href="{{ facebook }}" alt="{{ title }}" title="{{ title }}" target="_blank">Facebook</a>
{% elif bronze %}
<a class="bronzesponsorwebsite" href="{{ facebook }}" alt="{{ title }}" title="{{ title }}" target="_blank">Facebook</a>
{% endif %}
</div>
{% endif %}


{% if address %}
<div class="map pad2remtop"><p><strong>Address :</strong> {{ address }}</p></div>
{% endif %}
{% if phone %}
<div class="map pad2remtop"><p><strong>Phone :</strong> {{ phone }}</p></div>
{% endif %}

{% if map %}
<div class="map pad2remtop"><p><strong>Map Shortcode :</strong> <a href="{{ map }}" title="{{ title }}">{{ map }}</a></p></div>
{% endif %}

</article>
</div>
 
<div class="item blogdetail">

{% if gold %}
<div class="goldsponsorsumimage">
<div class="blogdetailimage" align="center"><img src="{{ image }}?tr=w-400,h-400" alt="{{ title }}" title="{{ title }}"></div>
</div>
{% elif silver %}
<div class="silversponsorsumimage">
<div class="blogdetailimage" align="center"><img src="{{ image }}?tr=w-400,h-400" alt="{{ title }}" title="{{ title }}"></div>
</div>
{% elif bronze %}
<div class="bronzesponsorsumimage">
<div class="blogdetailimage" align="center"><img src="{{ image }}?tr=w-400,h-400" alt="{{ title }}" title="{{ title }}"></div>
</div>
{% endif %}
</div>

</div>
