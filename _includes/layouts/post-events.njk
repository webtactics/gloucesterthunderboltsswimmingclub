---
layout: layouts/grid-default.njk
section: post
---

<div class="detailwrapper">
    <article>
        <div class="grid-2">
            <div class="item postdetail">

 {% if results %}
                    <div class="eventresultspdf">


                        <a class="resultspdf"
                           href="{{ results }}"
                           title="{{ title }}"
                           alt="{{ title }}"
                           target="_blank">Event Results Pdf &gt;</a>
                    </div>
                    {% endif %}

 {% if rain %}
        <div class="cancelled">
<h4 class="sum-title-cancelled-no-radius">Event cancelled due to rain</h4>
</div>
                    {% endif %}

  <div class="detailcontent">{{ content | safe }}</div>
              
                <div class="eventdate">
                    <p>
                        <strong>Event Date :</strong>
                        {{ eventdate.day }}
                        {{ eventdate.month }}, {{ eventdate.year }}
                    </p>
                </div>
                <div class="time">
                    <p>
                        <strong>Time :</strong>
                        {{ time }}
                    </p>
                </div>
                <div class="where">
                    <p>
                        <strong>Where :</strong>
                        {{ where }}
                    </p>
                </div>
                <div class="map">
                    <p>
                        <strong>Map Shortcode :</strong>
                        <a href="{{ map }}" title="{{ title }}">{{ map }}</a>
                    </p>
                </div>
                {% if club %}
                    <p>
                        <strong>Hosting Club :</strong>
                        <a href="{{ clubURL }}"
                           title="Hosting Club : {{ title }}"
                           alt="Hosting Club : {{ title }}"
                           target="_blank">{{ club }}</a>
                    </p>
                {% endif %}
                {% if meetflyerpdf %}
                    <p>
                        <strong>Event Flyer:</strong>
                        <a href="{{ meetflyerpdf }}"
                           title="{{ title }} Flyer"
                           alt="{{ title }} Flyer"
                           target="_blank">{{ subtitle }} Pdf</a>
                    </p>
                {% endif %}
                {% if entrylink %}
                    <p>
                        <strong>Entry:</strong>
                        <a href="{{ entrylink }}"
                           title="{{ title }} entry"
                           alt="{{ title }} entry"
                           target="_blank">{{ subtitle }} entry</a>
                    </p>
                {% endif %}
                {% if events %}
                    <div class="eventsheld">
                        <p>Events to be held</p>
                    </div>
                {% endif %}
                <div class="grid-1x2event">


                    {% if freestyle %}
                        <div class="strokeleft">
                            <p>
                                <span class="stroketitle">Freestyle:</span>
                            </p>
                        </div>
                      
                              <div class="grid-1x4eventlength">
                                {% if freestyle.length25 %}<div class="strokeright"><span class="eventlength">25 metres</span></div>{% endif %}
                                {% if freestyle.length50 %}<div class="strokeright"><span class="eventlength">50 metres</span></div>{% endif %}
                                {% if freestyle.length100 %}<div class="strokeright"><span class="eventlength">100 metres</span></div>{% endif %}
                                {% if freestyle.length200 %}<div class="strokeright"><span class="eventlength">200 metres</span></div>{% endif %}
                                {% if freestyle.length400 %}<div class="strokeright"><span class="eventlength">400 metres</span></div>{% endif %}
                                {% if freestyle.length800 %}<div class="strokeright"><span class="eventlength">800 metres</span></div>{% endif %}
                                {% if freestyle.length1500 %}<div class="strokeright"><span class="eventlength">1500 metres</span></div>{% endif %}
                       </div>
                    {% endif %}

</div>
  <div class="grid-1x4event">

                    {% if backstroke %}
                        <div class="strokeleft">
                            <p>
                                <span class="stroketitle">Backstroke:</span>
                            </p>
                        </div>
                      
                       
                                {% if backstroke.length25 %}<div class="strokeright"><span class="eventlength">25 metres</span></div>{% endif %}
                                {% if backstroke.length50 %}<div class="strokeright"><span class="eventlength">50 metres</span></div>{% endif %}
                                {% if backstroke.length100 %}<div class="strokeright"><span class="eventlength">100 metres</span></div>{% endif %}
                                {% if backstroke.length200 %}<div class="strokeright"><span class="eventlength">200 metres</span></div>{% endif %}
                                {% if backstroke.length400 %}<div class="strokeright"><span class="eventlength">400 metres</span></div>{% endif %}
                                {% if backstroke.length800 %}<div class="strokeright"><span class="eventlength">800 metres</span></div>{% endif %}
                            
                       
                        
                    {% endif %}
                    </div>
                     <div class="grid-1x4event">
                    {% if breaststroke %}
                        <div class="strokeleft">
                            <p>
                                <span class="stroketitle">Breaststroke:</span>
                            </p>
                        </div>
                        
                         
                                {% if breaststroke.length25 %}<div class="strokeright"><span class="eventlength">25 metres</span></div>{% endif %}
                                {% if breaststroke.length50 %}<div class="strokeright"><span class="eventlength">50 metres</span></div>{% endif %}
                                {% if breaststroke.length100 %}<div class="strokeright"><span class="eventlength">100 metres</span></div>{% endif %}
                                {% if breaststroke.length200 %}<div class="strokeright"><span class="eventlength">200 metres</span></div>{% endif %}
                                {% if breaststroke.length400 %}<div class="strokeright"><span class="eventlength">400 metres</span></div>{% endif %}
                       
                      
                    {% endif %}
                    </div>
                     <div class="grid-1x4event">
                    {% if butterfly %}
                        <div class="strokeleft">
                            <p>
                                <span class="stroketitle">Butterfly:</span>
                            </p>
                        </div>
                        
                          
                                {% if butterfly.length25 %}<div class="strokeright"><span class="eventlength">25 metres</span></div>{% endif %}
                                {% if butterfly.length50 %}<div class="strokeright"><span class="eventlength">50 metres</span></div>{% endif %}
                                {% if butterfly.length100 %}<div class="strokeright"><span class="eventlength">100 metres</span></div>{% endif %}
                                {% if butterfly.length200 %}<div class="strokeright"><span class="eventlength">200 metres</span></div>{% endif %}
                                {% if butterfly.length400 %}<div class="strokeright"><span class="eventlength">400 metres</span></div>{% endif %}
                                {% if butterfly.length800 %}<div class="strokeright"><span class="eventlength">800 metres</span></div>{% endif %}
                        
                        
                    {% endif %}
                    </div>
                     <div class="grid-1x4event">
                    {% if medley %}
                        <div class="strokeleft">
                            <p>
                                <span class="stroketitle">IM:</span>
                            </p>
                        </div>
                       
                         
                                {% if medley.length100 %}<div class="strokeright"><span class="eventlength">100 metres</span></div>{% endif %}
                                {% if medley.length200 %}<div class="strokeright"><span class="eventlength">200 metres</span></div>{% endif %}
                                {% if medley.length400 %}<div class="strokeright"><span class="eventlength">400 metres</span></div>{% endif %}
                                {% if medley.length800 %}<div class="strokeright"><span class="eventlength">800 metres</span></div>{% endif %}
                          
                       
                    {% endif %}
                    </div>
                     <div class="grid-1x4event">
                    {% if relay %}
                        <div class="strokeleft">
                            <p>
                                <span class="stroketitle">Relay:</span>
                            </p>
                        </div>
                   
                      
                                {% if relay.length4x50 %}<div class="strokeright"><span class="eventlength">4 x 50 metres</span></div>{% endif %}
                                {% if relay.length4x100 %}<div class="strokeright"><span class="eventlength">4 x 100 metres</span></div>{% endif %}
                        
                      
                    {% endif %}
</div>



                    {% if distance %}
                        <p>
                            <strong>Distance</strong>
                        </p>
                    {% endif %}
                    {% if sprint %}
                        <p>
                            <strong>Sprint</strong>
                        </p>
                    {% endif %}
                    {% if team %}
                        <p>
                            <strong>Team</strong>
                        </p>
                    {% endif %}
                    {% if individual %}
                        <p>
                            <strong>Individual</strong>
                        </p>
                    {% endif %}
                    {% if mixed %}
                        <p>
                            <strong>Mixed</strong>
                        </p>
                    {% endif %}
                    {% if interclub %}
                        <p>
                            <strong>Interclub</strong>
                        </p>
                    {% endif %}
                    {% if invitational %}
                        <p>
                            <strong>Invitational</strong>
                        </p>
                    {% endif %}
                    {% if championship %}
                        <p>
                            <strong>Championship</strong>
                        </p>
                    {% endif %}

                    

       {% if results %}
                    <div class="eventresultspdf">
                        <a class="resultspdf"
                           href="{{ results }}"
                           title="{{ title }}"
                           alt="{{ title }}"
                           target="_blank">Event Results Pdf &gt;</a>
                    </div>
                    {% endif %}


 {% if numberswims %}

 <div class="tworemdotteddivider"></div>
 <div class="grid-1x3"> 
    
    <div class="strokeleft">
        <p><span class="stroketitle">Max Swims:</span></p>
    </div>

    <div class="strokeright"><p>{{ numberswims }}</p>
    </div>
    
</div>
{% endif %}



{% include "components/global/tagged.html" %}



                
            </div>
            
            <div class="sidebarwrapper">
 {% include "components/maps/index.html" %}

                
                                                </div>
                                            </div>
                                        </article>
                                    </div>
